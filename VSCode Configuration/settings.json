{
	"workbench.startupEditor": "none",
	"window.controlsStyle": "custom",
	"workbench.productIconTheme": "fluent-icons",
	"workbench.iconTheme": "catppuccin-mocha",
	"editor.fontFamily": "JetBrainsMono Nerd Font",
	"workbench.layoutControl.enabled": false,
	"editor.renderWhitespace": "none",
	"editor.renderControlCharacters": false,
	"editor.stickyScroll.enabled": false,
	"breadcrumbs.enabled": false,
	"editor.minimap.renderCharacters": false,
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"editor.wordWrap": "on",
	"files.autoSave": "afterDelay",
	"git.confirmSync": false,
	"git.enableSmartCommit": true,
	"git.autofetch": true,
	"editor.formatOnSave": true,
	"editor.formatOnPaste": true,
	"editor.defaultFormatter": "biomejs.biome",
	"cSpell.userWords": [
        "consensia",
        "Enneagram",
        "frontmatter",
        "frontmatters",
        "HÄ“sperus",
        "Hesprs",
        "Libertas",
        "lifecycles",
        "lovership",
        "microkernel",
        "omnikernel",
        "Pointeract",
        "prerender",
        "prerendered",
        "prerendering",
        "Shadcn",
        "subfolders",
        "Tauri",
        "Techstack",
        "techstacks",
        "Vite"
    ],
	"terminal.integrated.stickyScroll.enabled": false,
	"[nix]": {
		"editor.defaultFormatter": "jnoortheen.nix-ide"
	},
	"[rust]": {
		"editor.defaultFormatter": "rust-lang.rust-analyzer"
	},
	"[java]": {
		"editor.defaultFormatter": "redhat.java"
	},
    "[vue]": {
        "editor.codeActionsOnSave": {
            "source.fixAll.biome": "never"
        },
        "editor.formatOnPaste": false
    },
	"security.workspace.trust.untrustedFiles": "open",
	"workbench.navigationControl.enabled": false,
	"chat.disableAIFeatures": true,
	"workbench.settings.showAISearchToggle": false,
	"workbench.editor.editorActionsLocation": "hidden",
	"editor.quickSuggestionsDelay": 0,
	"search.searchView.keywordSuggestions": true,
	"diffEditor.experimental.useTrueInlineView": true,
	"editor.inlineSuggest.experimental.showOnSuggestConflict": "whenSuggestListIsIncomplete",
	"editor.inlineSuggest.minShowDelay": 0,
	"terminal.integrated.enableMultiLinePasteWarning": "never",
    "terminal.integrated.tabs.location": "left",
	"workbench.editor.empty.hint": "hidden",
	"workbench.secondarySideBar.showLabels": false,
	"Lingma.DisplayLanguage": "English",
	"Lingma.PreferredLanguage for AI Chat": "English",
	"Lingma.PreferredLanguage forCommitMessage": "English",
	"terminal.integrated.gpuAcceleration": "on",
	"editor.codeActionsOnSave": {
		"source.organizeImports.biome": "explicit",
		"source.fixAll.biome": "explicit"
	},
	"biome.suggestInstallingGlobally": false,
	"Lingma.aI Chat.webToolsInAsk/AgentMode": "Auto-execute",
	"redhat.telemetry.enabled": false,
	"editor.minimap.size": "fill",
	"Lingma.methodQuickOperation": false,
	"explorer.fileNesting.enabled": true,
	"editor.unicodeHighlight.includeStrings": false,
	"terminal.integrated.cursorStyle": "line",
	"terminal.integrated.enableImages": true,
	"terminal.integrated.fontLigatures.enabled": true,
	"editor.fontLigatures": true,
	"editor.fontSize": 13,
	"window.confirmSaveUntitledWorkspace": false,
	"git.replaceTagsWhenPull": true,
	"editor.experimental.treeSitterTelemetry": true,
	"diffEditor.experimental.showMoves": true,
	"extensions.experimental.deferredStartupFinishedActivation": true,
	"typescript.experimental.useTsgo": true,
    "editor.detectIndentation": false,
    "typescript.updateImportsOnFileMove.enabled": "always",
    "editor.inertialScroll": true,
    "files.associations": {
        "*.canvas": "json"
    },
    "javascript.updateImportsOnFileMove.enabled": "always",
    "terminal.integrated.tabs.enabled": false,
    "editor.selectionClipboard": false,
}
